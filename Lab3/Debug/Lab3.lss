                                 ;==============================================================================
                                 ; Delay of 1 s (including RCALL)
                                 ; Using 16 clock cycles
                                 ;==============================================================================
                                 delay_1_micros:		//RCALL to this subroutine takes 4 cycles
000104 0000                      	NOP			
000105 0000                      	NOP			// add a number of NOP instructions to make the delay 1us. CPU is 16Mhz
000106 0000                      	NOP			// needing a totalt of 16 instructions including NOP to delay the CPU for 1 micro second
000107 0000                      	NOP
000108 0000                      	NOP
000109 0000                      	NOP
00010a 0000                      	NOP
00010b 0000                      	NOP			//total of 8 NOP because 4+4+8 = 16 cycles
                                 
                                 
00010c 9508                      	RET			//4 cycles
                                 			
                                 ;==============================================================================
                                 ; Delay of X s
                                 ;	With excess cost of 8 cycles (LDI + RCALL = 4 cycles, RET = 4 cycles)
                                 ; Uses registers:
                                 ;	R24				Input parameter data (X s)
                                 ;==============================================================================
                                 delay_micros:						// a total of 16 clockcycles each loop iteration
                                 									// calling method will gain a total cycles of (16*x)+8 for RET + LDI+RCALL
                                 	Loop_micros:					// start label to jump to in loop
00010d 0000                      		NOP							; 13 NOP
00010e 0000                      		NOP
00010f 0000                      		NOP
000110 0000                      		NOP
000111 0000                      		NOP
000112 0000                      		NOP
000113 0000                      		NOP
000114 0000                      		NOP
000115 0000                      		NOP
000116 0000                      		NOP
000117 0000                      		NOP							
000118 0000                      		NOP	
000119 0000                      		NOP															
                                 												
00011a 958a                      		DEC R24						// Will Loop x-times				;1 cycle
00011b f789                      		BRNE Loop_micros			//									;2 if jump, else 1 cycle
00011c 9508                      	RET								// We get here when R24 is == 0		;4 cycles
                                 
                                 ;==============================================================================
                                 ; Delay of X ms
                                 ;	With excess cost of 8 cycles (LDI + RCALL = 4 cycles, RET = 4 cycles)
                                 ; Uses registers:
                                 ;	R18				Copy of parameter data (X ms)
                                 ;	R24				Input parameter data (X ms) and
                                 ;					also input to 'delay_micros'.
                                 ;==============================================================================
                                 delay_ms:							
00011d 2f28                      	MOV R18, R24					//Do the outer loop x times as specified in input parameter R24
                                 	L1_ms:
00011e ef8a                      		LDI R24, 0xFA				//calling delay_micros with parameter 250, with a total of 4 times to get 1 ms
00011f dfed                      		RCALL delay_micros			;(16*250)+4 cycles
000120 ef8a                      		LDI R24, 0xFA				
000121 dfeb                      		RCALL delay_micros			;(16*250)+4 cycles
000122 ef8a                      		LDI R24, 0xFA				
000123 dfe9                      		RCALL delay_micros			;(16*250)+4 cycles
000124 ef8a                      		LDI R24, 0xFA				
000125 dfe7                      		RCALL delay_micros			;(16*250)+4 cycles
000126 952a                      		DEC R18
000127 f7b1                      		BRNE L1_ms
000128 9508                      	RET
                                 
                                 
                                 
                                 ;==============================================================================
                                 ; Delay of 1 second
                                 ; Uses Registers:
                                 ;	R24 - Parameter to delay_ms subroutine
                                 ;==============================================================================
                                 delay_1_s:						
                                 	
000129 ef8a                      	LDI R24, 0xFA			//calling delay_ms with parameter 250, with a total of 4 times to get 1 s
00012a dff2                      	RCALL delay_ms
00012b ef8a                      	LDI R24, 0xFA			
00012c dff0                      	RCALL delay_ms
00012d ef8a                      	LDI R24, 0xFA			
00012e dfee                      	RCALL delay_ms
00012f ef8a                      	LDI R24, 0xFA			
000130 dfec                      	RCALL delay_ms
000131 9508                      	RET
                                 
                                 
                                 
                                 ;==============================================================================
                                 ; Delay for x seconds
                                 ; Uses Registers:
                                 ;	R19  - Copy of parameter data (x seconds)
                                 ;	R24  - For Parameter of x seconds
                                 ;==============================================================================
                                 delay_s:					//Calls delay_1_s x times
000132 2f38                      	MOV R19, R24
                                 	L1_s:
000133 dff5                      		RCALL delay_1_s
000134 953a                      		DEC R19
000135 f7e9                      		BRNE L1_s
000136 9508                      	RET
