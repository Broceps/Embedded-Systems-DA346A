                                 .EQU			ROLL_KEY		 =	0x02				//key for rolling the dice
                                 
                                 
                                 
                                 ;==============================================================================
                                 ; Roll a Dice
                                 ; Rolling a dice by checking if key 2 is pressed, 
                                 ; while pressed the dice is rolling and on release 
                                 ; the dice will be rolled, printing the result on the LDC
                                 ; Uses Registers:
                                 ;	R16(TEMP) - for storing and working with values of a dice
                                 ;
                                 ;==============================================================================
                                 roll_dice:
00037d e006                      	LDI TEMP, 6						//Load all the possible values of a dice
                                 	load:
00037e 0000                      		NOP
00037f 0000                      		NOP							//Keyboard driver needs two NOP's to read correctly from keyboard
000380 930f                      		PUSH TEMP					//Save temp to stack so it doesnt get destroyed in subroutine
000381 dd07                      		RCALL read_keyboard_dice			//get value from keyboard
000382 910f                      		POP TEMP					//Retrieve temp from stack
000383 3372                      		CPI RVAL, 50				//check if rolling-key is pressed down (i.e. key 2) with ASCII offset
000384 f009                      		BREQ roll					//if still pressed
000385 9508                      		RET							//else if key is released
                                 	roll:
000386 950a                      		DEC TEMP					//"Rotating" the dice by decrementing the value stored (from 6 -> 1)
000387 f3a9                      		BREQ roll_dice				//If we get to 0, we start again from top and reload the value 6 to the dice
000388 cff5                      		RJMP load					//Else keep rolling
                                 	.INCLUDE		"stat_data.inc"
                                 
                                  * stat_data.inc
                                  *
                                  * Contains references and operations to manipulate statistics from Dice throws. Stores number of throws and occurance
                                  * of each side of the dice in the RAM memory
                                  *
                                  * Created: 2020-12-07 20:29:10
                                  * Author: Robin Andersson
                                  */ 
                                 
                                 ;==============================================================================
                                 ; RAM allocation of stats
                                 ;==============================================================================
                                 .DSEG									//Wokring with RAM
                                 .ORG 0x200								//Set starting point address of data segment to 0x200
                                 statistical_data:
000200                           	.BYTE 7								//index 1-6 of the string is the frequency of each dice throw, index 0 is amount of throws. 
                                 .CSEG									//Go back to working with Program/flash-memory
                                 
                                 ;==============================================================================
                                 ; Operation to store statistics in the RAM-memory
                                 ; The function will increment the Total number of throws and the
                                 ; number of throws with results equals RVAL.
                                 ; Registers used:
                                 ;	RVAL - Input parameter containing the dice throw
                                 ;	X - Pointer to RAM memory
                                 ;	TEMP(R16) - for temporary values
                                 ;==============================================================================
                                 store_stat:
000389 e0b2                      	LDI XH, HIGH(statistical_data)	//assign pointer to the RAM memory
00038a e0a0                      	LDI XL, LOW(statistical_data)	//where we store our statistical data
                                 
                                 	//First we need to increment the number of throws (at index 0)
00038b 910c                      	LD TEMP, X						//Load the current value of throws into TEMP (already at index 0, no need for offset)
00038c 9503                      	INC TEMP						//Increment the value with one
00038d 930c                      	ST X, TEMP						//And save it
                                 
                                 	//Then we need to increment the value of the dice-throw (at index = RVAL)
00038e 0fa7                      	ADD XL, RVAL						//Offset of the value of the dice-throw to get to the correct index (1-6)
00038f e070                      	LDI RVAL, 0x00
000390 1fb7                      	ADC XH, RVAL
000391 910c                      	LD TEMP, X						//Load the current value into TEMP
000392 9503                      	INC TEMP						//Increment the value with one
000393 930c                      	ST X, TEMP						//And save it
                                 
000394 9508                      	RET
                                 
                                 ;==============================================================================
                                 ; Operation get statistics from the RAM-memory
                                 ; RVAL gives the value for which the result is retrieved. 
                                 ; If RVAL = 0, the total number of throws shall be returned. The result is returned in RVAL.
                                 ; Registers used:
                                 ;	RVAL - Input and Output parameter containing which stat is to be retrieved and returns the value
                                 ;	X - Pointer to RAM memory
                                 ;==============================================================================
                                 get_stat:
000395 e0b2                      	LDI XH, HIGH(statistical_data)	//assign pointer to the RAM memory
000396 e0a0                      	LDI XL, LOW(statistical_data)	//where we store our statistical data
                                 
000397 0fa7                      	ADD XL, RVAL						//Get the offset for the value we want to retrieve
000398 e070                      	LDI RVAL, 0x00
000399 1fb7                      	ADC XH, RVAL
                                 
00039a 917c                      	LD RVAL, X						//Load the value into RVAL
                                 	
00039b 9508                      	RET
                                 
                                 ;==============================================================================
                                 ; Operation clear all the statistics from the RAM-memory
                                 ; Clears all data
                                 ; Registers used:
                                 ;	X - Pointer to RAM memory
                                 ;	TEMP(R16) - for temporary values
                                 ;==============================================================================
                                 clear_stat:
00039c e0b2                      	LDI XH, HIGH(statistical_data)	//assign pointer to the RAM memory
00039d e0a0                      	LDI XL, LOW(statistical_data)	//where we store our statistical data
                                 
00039e e000                      	LDI TEMP, 0						//What we will be writing over with to reset memory
00039f 930d                      	ST X+, TEMP						//Store the reset-value and increment to go through all 7 slots
0003a0 930d                      	ST X+, TEMP
0003a1 930d                      	ST X+, TEMP
0003a2 930d                      	ST X+, TEMP
0003a3 930d                      	ST X+, TEMP
0003a4 930d                      	ST X+, TEMP
0003a5 930c                      	ST X, TEMP
                                 
0003a6 9508                      	RET
                                 
