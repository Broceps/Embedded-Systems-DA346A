                                 ;==============================================================================
                                 init:
0003a7 ef0f                      	LDI				TEMP,			LOW(RAMEND)		; Set stack pointer
0003a8 bf0d                      	OUT				SPL,			TEMP				; at the end of RAM.
0003a9 e201                      	LDI				TEMP,			HIGH(RAMEND)
